  "compilerOptions": {
    "outDir": "dist",
    "esModuleInterop": true,
    "lib": [
      "ESNext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2018",
    "composite": true
  },
  "include": [
    "src"
  ]
}
